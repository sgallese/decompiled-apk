    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/widget_bar_background"/>
            <corners android:radius="@dimen/bar_radius"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/hpColor"/>
                <corners android:radius="@dimen/bar_radius"/>
            </shape>
        </clip>
    </item>
</layer-list>
