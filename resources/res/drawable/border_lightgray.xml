<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-2dp"
        android:top="-2dp"
        android:right="-2dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#ff959595"/>
            <solid android:color="#00ffffff"/>
            <padding
                android:left="0dp"
                android:top="8dp"
                android:right="0dp"
                android:bottom="8dp"/>
        </shape>
    </item>
</layer-list>
